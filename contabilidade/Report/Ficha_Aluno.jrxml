	<queryString>
		<![CDATA[SELECT C5.*,
(SELECT C1_DESCR FROM SC101 WHERE C1_ID = C5.C5_IDATIVIDADE AND C1_FILIAL = '00') AS ATIVIDADE,
(SELECT C1_LIMITECH FROM SC101 WHERE C1_ID = C5.C5_IDATIVIDADE AND C1_FILIAL = '00') AS LIMITECH,
(SELECT C2_DESCR FROM SC201 WHERE C2_ID = C5.C5_IDEVENTO AND C2_FILIAL = '00') AS EVENTO,
(SELECT C4_DESCR FROM SC401 WHERE C4_ID = C5.C5_IDINSTITUICAO AND C4_FILIAL = '00') AS INSTIT,
(SELECT AA_IDTURMA FROM SAA01 WHERE AA_IDALUNO = C5_IDALUNO
 AND AA_ANO = $P{i_ano} AND AA_SEMESTRE = $P{i_sem} AND AA_MODAL = 'N' AND AA_FILIAL = '00') AS TURMA,
(SELECT A1_MATRICULA FROM SA101 WHERE A1_ID = C5.C5_IDALUNO) AS MAT,
(SELECT A1_NOME FROM SA101 WHERE A1_ID = C5.C5_IDALUNO) AS ALUNO
FROM SC501 C5
WHERE C5_IDALUNO = $P{i_idaluno}
AND C5_FILIAL = '00'
ORDER BY C5.C5_IDATIVIDADE]]>
