	<queryString>
		<![CDATA[select
  (SELECT A1_MATRICULA FROM SA101 WHERE A1_ID = AA_IDALUNO) AS MAT,
  (SELECT A1_NOME FROM SA101 WHERE A1_ID = AA_IDALUNO) AS ALUNO,AA_IDTURMA,
(select sum(
  case
  when (TCH1 > LIMIT) then
  LIMIT
  when (TCH1 <= LIMIT) then TCH1 end
  ) as TCH
  from
  (SELECT SUM(C5.C5_CH) AS TCH1, C1.C1_LIMITECH AS LIMIT FROM sc501 C5, sc101 C1
  where c5.c5_idatividade = c1.c1_id
  and c5.c5_filial = c1.c1_filial
  and c5.c5_idaluno = aa_idaluno
  AND C5.c5_filial = '00'
  group BY C5.c5_idaluno, C5.c5_idatividade, C1.C1_LIMITECH) as c6) AS TCH
FROM SAA01
WHERE AA_FILIAL = '00'
AND AA_ANO = $P{i_ano}
AND AA_SEMESTRE = $P{i_sem}
AND AA_IDTURMA = $P{i_idturma}
AND AA_MODAL = 'N'
ORDER BY 2]]>
	</queryString>
